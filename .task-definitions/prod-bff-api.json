{
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "inferenceAccelerators": [],
  "containerDefinitions": [
    {
      "name": "service-bff-api",
      "image": "",
      "essential": true,
      "environment": [
        {
          "name": "ASPNETCORE_ENVIRONMENT",
          "value": "Production"
        },
        {
          "name": "CustomerApiBaseAddress",
          "value": "http://customers-api.alhardynet.local"
        }],
      "portMappings": [{
        "hostPort": 80,
        "protocol": "tcp",
        "containerPort": 80
      }]
    }
  ],
  "volumes": [],
  "networkMode": "awsvpc",
  "family": "service-bff",
  "memory": "512",
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::078110025121:role/EcsClusteralhardynetDefaultTaskRole",
  "taskRoleArn": "arn:aws:iam::078110025121:role/EcsClusteralhardynetDefaultTaskRole",
  "placementConstraints": []
}